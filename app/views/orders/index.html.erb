<div class="sign_up_whole">
  <div >
    <h4>Available Orders:</h4>
    <% unless @orders.empty? %>
      <ul>
        <% @orders.each do |order| %>
          <% if !order.fulfilled && order.consumer != @user %>
            <li>
              <%=link_to "#{order.cookie_amount} #{order.cookie_type}", "/orders/#{order.id}" %>
            </li>
          <%end%>
        <% end %>
      </ul>
    <% end %>
  </div>
  <p><%= link_to "Back to Home ", users_path %></p>

</div>
